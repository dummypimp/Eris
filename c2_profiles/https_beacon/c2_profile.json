{
  "name": "https_beacon",
  "author": "@RedTeam",
  "description": "HTTPS beacon C2 profile for Eris Android Agent with malleable profiles and domain fronting support",
  "version": "1.0.0",
  "is_p2p": false,
  "is_server_routed": true,
  "parameters": [
    {
      "name": "callback_host",
      "description": "Callback Host",
      "default_value": "https://domain.com",
      "verifier_regex": "^https://[a-zA-Z0-9]+",
      "required": true,
      "randomize": false,
      "format_string": "",
      "parameter_type": "String"
    },
    {
      "name": "callback_port",
      "description": "Callback Port",
      "default_value": 443,
      "verifier_regex": "^[0-9]+$",
      "required": true,
      "randomize": false,
      "format_string": "",
      "parameter_type": "Number"
    },
    {
      "name": "encrypted_exchange_check",
      "description": "Perform Key Exchange",
      "default_value": true,
      "required": false,
      "parameter_type": "Boolean"
    },
    {
      "name": "callback_interval",
      "description": "Callback Interval (seconds)",
      "default_value": 10,
      "verifier_regex": "^[0-9]+$",
      "required": false,
      "randomize": false,
      "format_string": "",
      "parameter_type": "Number"
    },
    {
      "name": "callback_jitter",
      "description": "Callback Jitter (0-100)",
      "default_value": 23,
      "verifier_regex": "^[0-9]+$",
      "required": false,
      "randomize": false,
      "format_string": "",
      "parameter_type": "Number"
    },
    {
      "name": "user_agent",
      "description": "User Agent String",
      "default_value": "Mozilla/5.0 (Linux; Android 14; SM-G998B) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/120.0.0.0 Mobile Safari/537.36",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "domain_front",
      "description": "Domain for domain fronting",
      "default_value": "",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "killdate",
      "description": "Kill Date (YYYY-MM-DD)",
      "default_value": "2025-12-31",
      "verifier_regex": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$",
      "required": false,
      "parameter_type": "Date"
    },
    {
      "name": "headers",
      "description": "Additional HTTP headers (JSON format)",
      "default_value": "{\"Accept\": \"application/json\", \"Content-Type\": \"application/json\"}",
      "required": false,
      "parameter_type": "Dictionary"
    },
    {
      "name": "get_uri",
      "description": "GET request URI",
      "default_value": "/api/v1/beacon",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "post_uri", 
      "description": "POST request URI",
      "default_value": "/api/v1/submit",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "proxy_host",
      "description": "Proxy Host",
      "default_value": "",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "proxy_port",
      "description": "Proxy Port",
      "default_value": 8080,
      "required": false,
      "parameter_type": "Number"
    },
    {
      "name": "proxy_user",
      "description": "Proxy Username",
      "default_value": "",
      "required": false,
      "parameter_type": "String"
    },
    {
      "name": "proxy_pass",
      "description": "Proxy Password",
      "default_value": "",
      "required": false,
      "parameter_type": "String"
    }
  ]
}
